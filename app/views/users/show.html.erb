<h1><%= @user.name %></h1>

<% if @user.plans.length > 0 %>
  <h2>Your plans</h2>
  <% @user.plans.each do |pl| %>
  <h1> Plan for <%= pl.date %>, go to <%= link_to pl.venue.name, plan_path(pl) %></h1>
  <% end %>
<% else %>
  <h2> You don't have any plans! </h2>
<% end %>

<h2><%= link_to 'make a date with destiny', venues_start_path %></h2>

<%= link_to 'edit user', edit_user_path(@user) %>
