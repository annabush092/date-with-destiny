<%  if @plan.errors.any? %>
  <% @plan.errors.full_messages.each do |m| %>
  <%= m %><br>
  <% end %>
<% end %>

<%= form_for @plan do |f| %>
  <%= f.label :date %>
  <%= f.date_field :date %>
  <%= f.submit %>
<% end %>



<p> Your venue is: <%= @plan.venue.name %> </p>

<h1><%= link_to 'DEFY YOUR FATE', new_plan_path %></h1>
