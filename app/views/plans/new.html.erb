<%  if @plan.errors.any? %>
  <% @plan.errors.full_messages.each do |m| %>
  <%= m %><br>
  <% end %>
<% end %>

<%= form_tag plans_path, method: "post" do %>

  <%= label_tag 'zipcode' %>
  <%= number_field_tag 'zipcode' %>

  <%= label_tag 'date' %>
  <%= date_field_tag 'date' %>

  <%= submit_tag 'find your destined destination' %>

<% end %>

<%= link_to 'defy your fate', new_plan_path %>
